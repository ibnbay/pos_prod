  <div class="nested-fields">
    <div class="form-group">
      <%= f.label :barang_id, class: "col-sm-3 control-label" %>
      <div class="col-sm-9">
        <%= f.collection_select :barang_id, @barangs, :id, :nama, {}, {class: "form-control"} %>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :jumlah, class: "col-sm-3 control-label" %>
      <div class="col-sm-3">
        <%= f.number_field :jumlah, class: "form-control" %>
      </div>

      <%= f.label :amount, class: "col-sm-2 control-label" %>
      <div class="col-sm-4">
        <%= f.number_field :amount, class: "form-control" %>
      </div>
    </div>
    <div class="form-group">
      <label for="total" class="col-sm-3 control-label">Total</label>
      <div class="col-sm-5">
        <input type="text" class="form-control" value="50000" disabled="disabled">
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-4">
        <%= link_to_remove_association "Kurang", f, class: "btn btn-danger"  %>
      </div>
    </div>
  </div>
